{
  "name": "@sitearcade/styled-theme",
  "version": "2.5.2",
  "description": "Theme-related react hooks, components, and utils.",
  "license": "MIT",
  "homepage": "https://www.sitearcade.com",
  "repository": "github:sitearcade/styled-theme",
  "bugs": "https://github.com/sitearcade/styled-theme/issues",
  "author": {
    "name": "Mike Stop Continues",
    "email": "mike@sitearcade.com",
    "url": "https://www.sitearcade.com"
  },
  "sideEffects": false,
  "source": "src/index.js",
  "main": "./dist/index.js",
  "jsnext:main": "./dist/index.modern.js",
  "module": "./dist/index.module.js",
  "syntax": {
    "esmodules": "./dist/index.modern.js"
  },
  "exports": {
    ".": {
      "browser": "./dist/index.module.js",
      "import": "./dist/index.module.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "microbundle watch --no-compress -f cjs,esm,modern --jsx React.createElement --jsxImportSource react",
    "build": "microbundle --no-compress -f cjs,esm,modern --jsx React.createElement --jsxImportSource react",
    "clean": "rm -rf dist",
    "test": "NODE_ENV=test jest --passWithNoTests --coverage",
    "test:ci": "NODE_ENV=test jest --passWithNoTests --ci",
    "test:watch": "NODE_ENV=test jest --passWithNoTests --coverage --watch",
    "preversion": "npm run build && npm test",
    "postversion": "npm publish --access public",
    "prepublishOnly": "npm run clean && npm run build",
    "postpublish": "git push origin --all; git push origin --tags",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@csstools/convert-colors": "^2.0.0",
    "@sitearcade/react-hooks": "^0.4.1",
    "polished": "^4.1.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@sitearcade/babel-preset": "1.1.10",
    "@sitearcade/browserslist-config": "0.3.1",
    "@sitearcade/eslint-config": "1.4.1",
    "@sitearcade/jest-preset": "2.3.8",
    "@storybook/addon-actions": "6.2.7",
    "@storybook/addon-docs": "6.2.7",
    "@storybook/addon-essentials": "6.2.7",
    "@storybook/addon-links": "6.2.7",
    "@storybook/react": "6.2.7",
    "@testing-library/dom": "7.30.3",
    "babel-loader": "8.2.2",
    "microbundle": "0.13.0",
    "npm-run-all": "4.1.5",
    "raf-stub": "3.0.0",
    "storybook-addon-performance": "0.15.2"
  },
  "engines": {
    "node": ">=10"
  },
  "browserslist": [
    "extends @sitearcade/browserslist-config"
  ],
  "babel": {
    "presets": [
      "module:@sitearcade/babel-preset"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@sitearcade/eslint-config"
    ]
  },
  "jest": {
    "preset": "@sitearcade/jest-preset"
  }
}
